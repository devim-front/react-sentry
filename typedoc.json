{
  "inputFiles": ["./src"],
  "mode": "file",
  "exclude": ["**/*+(index|.spec|.e2e|.test).(ts|tsx)"],
  "readme": "none",
  "excludeNotDocumented": true,
  "excludeNotExported": true,
  "excludePrivate": true,
  "excludeProtected": true,
  "stripInternal": true,
  "hideGenerator": true,
  "hideSources": true,
  "theme": "bitbucket",
  "skipSidebar": true,
  "plugin": ["typedoc-plugin-markdown"]
}
